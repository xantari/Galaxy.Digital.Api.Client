<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>https://github.com/xantari</Authors>
    <Company />
    <Description>Galaxy Digital API Client Library for .NET</Description>
    <RepositoryUrl>https://github.com/xantari/Galaxy.Digital.Api.Client</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageProjectUrl>https://github.com/xantari/Galaxy.Digital.Api.Client</PackageProjectUrl>
    <PackageTags>Galaxy Digital</PackageTags>
    <PackageIcon>GalaxyDigitalIcon-270x270.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Assets\GalaxyDigitalIcon-270x270.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
